<!-- TREE --> 
		<record model="ir.ui.view" id="openhealth.management_view_tree">
			<field name="name">openhealth.management.tree</field>
			<field name="model">openhealth.management</field>
			<field name="arch" type="xml">

				<tree string="Management Tree"

										colors="
										blue:pl_max==True;
										red:pl_min==True
										"

					>


					<field name="name"/>

					<field name="configurator"  string="Config - 07-12"	/>

					<field name="write_date"        string="Actualizado"	groups="openhealth.roots"/>



					<field name="rsp_count_delta"	groups="openhealth.roots"/>
					<field name="rsp_total_delta"	groups="openhealth.roots"/>


					<field name="per_amo_families"  	string="% Mto Fam"		widget="percentage"	groups="openhealth.roots"/>
					<field name="per_amo_subfamilies"  	string="% Mto Sub Fam"	widget="percentage"	groups="openhealth.roots"/>


					<field name="month"  	groups="openhealth.roots"/>
					<field name="year"   	groups="openhealth.roots"/>
					
					<field name="owner"         	groups="openhealth.roots"/>



					<field name="pl_max"/>
					<field name="pl_min"/>


					<field name="total_amount"      sum="Total" widget="monetary"/>
<!--
					<field name="total_amount_year"/>
-->
					<field name="per_amo_total"     string="Porcentaje Anual"		widget="percentage"  />



					<field name="type_arr"     	groups="openhealth.roots"/>


					<field name="nr_products"/>
					<field name="nr_consultations"/>
					<field name="nr_procedures"/>

					<field name="nr_credit_notes"/>
					<field name="nr_other"/>


					<field name="ratio_pro_con"     string="Ratio"/>
					
					<field name="total_tickets"/>
					<field name="total_count"/>


				</tree>
			</field>
		</record>

