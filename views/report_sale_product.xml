<?xml version="1.0"?>
<openerp>
	<data>
		


<!-- FORM -->
		<record model="ir.ui.view" id="report_sale_product_form_view">
			<field name="name">price_list.report.sale.product.form</field>
			<field name="model">openhealth.report.sale.product</field>
			<field name="inherit_id" ref="openhealth.rsp_form_view"/>
			<field name="mode">primary</field>
			<field name="priority" eval="1"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='total']" position="after">
						<field name="management_id"/>
				</xpath>
			</field>
		</record>


<!-- TREE --> 
		<record model="ir.ui.view" id="openhealth.rsp_tree">
			<field name="name">openhealth.report.sale.product.tree</field>
			<field name="model">openhealth.report.sale.product</field>
			<field name="arch" type="xml">
				<tree default_order='name desc'>
					<field name="name"/>
					<field name="management_id" />
					<field name="test_target"/>
					<field name="total_qty"/>
					<field name="total"/>
				</tree>
			</field>
		</record>




